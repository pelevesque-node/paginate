{
  "name": "@pelevesque/paginate",
  "version": "0.1.1",
  "description": "Makes useful calculations for creating website pagination links.",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "eslintConfig": {
    "globals": {
      "describe": "readonly",
      "it": "readonly"
    },
    "rules": {
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline"
        }
      ],
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "ObjectExpression": true,
            "ArrayExpression": true,
            "IfStatement": true
          }
        }
      ]
    }
  },
  "scripts": {
    "test": "standardx && mocha --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "standardx": "standardx",
    "unit": "mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pelevesque-node/paginate.git"
  },
  "keywords": [
    "pagination",
    "links"
  ],
  "author": "Pierre-Emmanuel LÃ©vesque <me@pelevesque.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pelevesque-node/paginate/issues"
  },
  "homepage": "https://github.com/pelevesque-node/paginate#readme",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.0.1",
    "standardx": "^5.0.0"
  }
}
